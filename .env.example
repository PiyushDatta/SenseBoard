# SenseBoard environment template
# Copy this file to .env before running: bun run start:web

# Runtime settings (ports/provider/models/review) live in `senseboard.config.toml`.

# Optional explicit client API URL. Leave empty to auto-discover localhost server ports.
EXPO_PUBLIC_SERVER_URL=

# Optional. Needed only for OpenAI features (Whisper/OpenAI model calls).
OPENAI_API_KEY=

# Optional. Needed only when Claude/Anthropic is used.
ANTHROPIC_API_KEY=
